# for function in ~/zsh/functions/*; do
#   source $function
# done

# Local config
[[ -f ~/local/.zshrc ]] && source ~/local/.zshrc

# Exports
[[ -f ~/.exports ]] && source ~/.exports

# aliases
[[ -f ~/.aliases ]] && source ~/.aliases

# Settings
[[ -f ~/.zsh/settings.zsh ]] && source ~/.zsh/settings.zsh
