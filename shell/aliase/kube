# Default
# https://kubernetes.io/docs/reference/kubectl/cheatsheet/
#
# Kubernetes & Kubectl aliases.
#

command -v kubectl > /dev/null && alias k=kubectl         # https://kubernetes.io/docs/reference/kubectl/overview/
command -v kubens > /dev/null && alias ks=kubens          # https://github.com/ahmetb/kubectx
command -v kubedecode > /dev/null && alias kd=kubedecode  # https://github.com/mveritym/kubedecode
command -v kubectx > /dev/null && alias kx=kubectx        # https://github.com/ahmetb/kubectx

if ! kubectl config current-context >/dev/null 2>&1; then
: # do nothing as not inside context
else
: # do nothing as we may switch to context an there is no need to realod
fi

## Health
alias k.h='kubectl get cs'
alias k.health="kubectl get --raw='/readyz?verbose"

ns_show_label() {
  # list all namespaces with labels
  # k get ns -l 'team' -L team
  kubectl get ns -l "${1}" -L "${1}"
}

## Resources
alias k.api='kubectl api-resources' # Print the supported API resources on the server.
alias k.apiw='kubectl api-resources -o wide' # Print the supported API resources on the server.

## Nodes
# sort nodes by timestamp
alias k.nstamp='kubectl get nodes --sort-by=.metadata.creationTimestamp'
# sort nodes by kubelet
alias k.nsort='kubectl get node --sort-by={.status.nodeInfo.kubeletVersion}'
# list only masters
alias k.master='kubectl get nodes --selector=node-role.kubernetes.io/master'
alias k.masters='k.master'
# list only workers
alias k.workers="kubectl get nodes --selector='!node-role.kubernetes.io/master'"
alias k.worker="k.workers"
# list nodes with taints
alias k.taint='kubectl get node -o custom-columns=NAME:.metadata.name,TAINT:.spec.taints[*].effect'
# list only node statuses
alias k.nready='kubectl get nodes -o custom-columns=STATUS:status.conditions[-1].type'
alias k.dnol='kubectl describe nodes -l'
alias k.gnol='kubectl get nodes -l'
alias k.dno='kubectl describe nodes'
alias k.gno='kubectl get nodes'
alias k.gnooyaml='kubectl get nodes -o=yaml'
alias k.gnoowide='kubectl get nodes -o=wide'
alias k.gnoojson='kubectl get nodes -o=json'
alias k.gnow='kubectl get nodes --watch'
alias k.gnowoyaml='kubectl get nodes --watch -o=yaml'
alias k.gnowojson='kubectl get nodes --watch -o=json'
alias k.gnowowide='kubectl get nodes --watch -o=wide'
alias k.gnooyamll='kubectl get nodes -o=yaml -l'
alias k.gnoowidel='kubectl get nodes -o=wide -l'
alias k.gnoojsonl='kubectl get nodes -o=json -l'
alias k.gnowl='kubectl get nodes --watch -l'
alias k.gnowoyamll='kubectl get nodes --watch -o=yaml -l'
alias k.gnowowidel='kubectl get nodes --watch -o=wide -l'
alias k.gnowojsonl='kubectl get nodes --watch -o=json -l'

# Services
# service with specific columns
alias k.svc-get='kubectl get svc -o=custom-columns=NAME:.metadata.name,IP:.spec.clusterIP,PORT:.spec.ports[*].targetPort'

# Config view
alias k.c='kubectl config view'
# validate with dex folder structure
alias k.cfg='la ~/.kube && la ~/.kube/kubens'
# get the password for the e2e user
alias k.cvp=kubectl config view -o jsonpath='{.users[?(@.name == "e2e")].user.password}'
alias k.cvu1="kubectl config view -o jsonpath='{.users[].name}'"   # display the first user
alias k.cvu="kubectl config view -o jsonpath='{.users[*].name}'"   # get a list of users
alias k.cs='kubectl config get-contexts'                           # display list of contexts
alias k.cc='kubectl config current-context'                        # display the current-context

## Images
alias k.img-n="kubectl get pods -o jsonpath='{..image}' | tr -s '[[:space:]]' '\n' | sort | uniq -c | sort -r"
alias k.img-a="kubectl get pods -o jsonpath='{..image}' -A | tr -s '[[:space:]]' '\n' | sort | uniq -c | sort -r"
alias k.img='k.imgn'
alias k.img-ns='kubectl get pods -o=jsonpath='"'"'{range .items[*]}{"\n"}{.metadata.name}{":\t"}{range .spec.containers[*]}{.image}{", "}{end}{end}'"'"' --namespace ' # +namespace
alias k.img-pod='kubectl get pods --all-namespaces -o=jsonpath='"'"'{range .items[*]}{"\n"}{.metadata.name}{":\t"}{range .spec.containers[*]}{.image}{", "}{end}{end}'"'"' | sort'

## Labels
alias k.labels-team-uniq="kubectl get pods -A -o jsonpath='{..labels.team}' | tr -s '[[:space:]]' '\n' | sort | uniq"

## Pods
alias k.pod-n='kubectl get pod -o=custom-columns=NS:.metadata.namespace,NAME:.metadata.name --all-namespaces'
alias k.pod-nod='kubectl get pod -o=custom-columns=NODE:.spec.nodeName,NAME:.metadata.name,NS:.metadata.namespace --all-namespaces'
alias k.pod-nodn='kubectl get pod -o=custom-columns=NODE:.spec.nodeName,NAME:.metadata.name'
alias k.pod-nodns='kubectl get pod -o=custom-columns=NODE:.spec.nodeName,NAME:.metadata.name --namespace ' # +namespace
alias k.pod-stnod='kubectl get pod -o=custom-columns=NAME:.metadata.name,STATUS:.status.phase,NODE:.spec.nodeName --all-namespaces'
# get burstable pods in all namespaces
alias k.burst='k get pods -A --output=custom-columns=NAME:.metadata.name,NAMESPACE:.metadata.namespace,QOS-CLASS:.status.qosClass | grep Burstable'
_kctl_pods_custom_columns() {
  # kubectl get pods -o=custom-columns=NAME:.metadata.name,APP:.metadata.labels.app
  kubectl get pods -o=custom-columns=NAME:.metadata.name,LABEL(${1}):.metadata.labels.${1}
}
alias k.pods-column="_kctl_pods_custom_columns" # + custom label name e.g. team
alias k.podips="kubectl get pod -o custom-columns=POD-NAME:.metadata.name,HOST-IP:.status.hostIP,POD-IP:.status.podIP,POD-IP(s):.status.podIPs[*].ip"

